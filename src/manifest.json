{
  "name": "Warrior Tab",
  "short_name": "WWTab",
  "description": "A customizable, tab extension built for westwood students by Qrules under GNU GPL v3, based on Tabbie.",
  "version": "0.1",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "permissions": [
    "identity",
  ],
  "optional_permissions": [
    "http://*/",
    "https://*/",

    "management",
    "bookmarks",
    "chrome://favicon/*",
    "topSites",
    "sessions",
    "tabs"
  ]
    
  },
  "manifest_version": 2,
  "icons": {
    "48": "img/icon_48.png",
    "64": "img/icon_64.png",
    "128": "img/icon_128.png"
  }
}
